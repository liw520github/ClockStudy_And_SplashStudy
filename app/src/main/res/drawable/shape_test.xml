<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:height="29dp" android:state_pressed="false">
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="#ffe1e1e1"  />
            <corners android:radius="14dp" />
        </shape>
    </item>

    <item android:height="29dp" android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="#FDE633"
                />
            <corners android:radius="14dp" />
        </shape>
    </item>

</selector>